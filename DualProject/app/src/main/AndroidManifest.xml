<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dualproject">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activities.UpdateActivity"
            android:parentActivityName=".Activities.EmployeeListActivity">
        </activity>
        <activity android:name=".Activities.EmployeeFormActivity"
            android:parentActivityName=".Activities.EmployeeListActivity"/>
        <activity android:name=".Activities.EmployeeListActivity" />


        <activity android:name=".Activities.SignUpActivity" />
        <activity android:name=".Activities.UpdateDeleteActivity"
            android:parentActivityName=".Activities.NoteHomeActivity"/>
        <activity android:name=".Activities.AddingNoteActivity"
            android:parentActivityName=".Activities.NoteHomeActivity"/>
        <activity android:name=".Activities.NoteHomeActivity"
            android:parentActivityName=".Activities.EmployeeListActivity"/>
        <activity android:name=".Activities.LogInActivity" />

        <activity android:name=".Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>